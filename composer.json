{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "backpack/base": "^1.1",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-elfinder": "^0.4.1",
        "codezero/laravel-unique-translation": "^1.1",
        "cviebrock/eloquent-sluggable": "^4.6",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "fideloper/proxy": "^4.0",
        "jenssegers/date": "^3.5",
        "laracasts/flash": "^3.0",
        "laravel/cashier": "^9.1",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.2",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.7",
        "nwidart/laravel-modules": "^5.0",
        "spatie/laravel-activitylog": "^3.2",
        "spatie/laravel-analytics": "^3.7",
        "spatie/laravel-backup": "^6.1",
        "spatie/laravel-honeypot": "^1.3",
        "spatie/laravel-menu": "^3.3",
        "spatie/laravel-permission": "^2.36",
        "spatie/laravel-sitemap": "^5.3",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-stripe-webhooks": "^1.2",
        "spatie/laravel-tags": "2.4.1",
        "spatie/laravel-translatable": "^4.0",
        "spatie/laravel-translation-loader": "^2.3",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
